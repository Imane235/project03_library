<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TestWSImplService WSDL service -->

  <service name="TestWSImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webservice.library.nmocs.fr"/>
      <parameter name="wsdlServiceElement" value="TestWSImplService"/>
      <parameter name="schemaQualified" value="http://webservice.library.nmocs.fr"/>
      <parameter name="wsdlServicePort" value="TestWSImpl"/>
      <parameter name="className" value="fr.nmocs.library.webservice.TestWSImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="TestWSImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getAddition" qname="operNS:getAddition" xmlns:operNS="http://webservice.library.nmocs.fr" returnQName="retNS:getAdditionReturn" xmlns:retNS="http://webservice.library.nmocs.fr" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://webservice.library.nmocs.fr" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://webservice.library.nmocs.fr" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getAddition"/>

  </service>
</deployment>
